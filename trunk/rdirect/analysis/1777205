    <script type="text/javascript">
  var ReferralRooster = {
    hideRooster: function(){
      jQuery.cookie('airbnb_referrals_rooster', 'hide', {expires: 90, path: '/'});
    },

    init: function(){
      jQuery('.rooster .close-button').click(function(){
        jQuery(this).parent().slideUp(function(){ jQuery('body').css('padding-top', 0); });
        ReferralRooster.hideRooster();
      });

      jQuery('#invite-button').click(function(){
        ReferralRooster.hideRooster();
      });
    }
  };

  jQuery(document).ready(function(){
    ReferralRooster.init();
  });
</script>

